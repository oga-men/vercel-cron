{
  "version": 2,
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9"
      }
    },
    {
      "src": "api/cron-export.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/cron-export.py"
    },
    {
      "src": "/(.*)",
      "dest": "api/main.py"
    }
  ],
  "functions": {
    "api/main.py": {
      "maxDuration": 30
    },
    "api/cron-export.py": {
      "maxDuration": 60
    }
  },
  "crons": [
    {
      "path": "/api/cron-export",
      "schedule": "*/15 * * * *"
    }
  ],
  "env": {
    "PYTHONPATH": "$PYTHONPATH:."
  }
}