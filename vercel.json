{
  "version": 2,
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/main.py"
    }
  ],
  "functions": {
    "api/main.py": {
      "maxDuration": 60
    }
  },
  "crons": [
    {
      "path": "/api/cron-export",
      "schedule": "*/15 * * * *"
    }
  ]
}